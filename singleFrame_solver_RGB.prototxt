net: "train_test_singleFrame_RGB.prototxt"
test_iter: 2 
test_state: { stage: 'test-on-test' }
test_interval: 10
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 3000
display: 1
max_iter: 500
momentum: 0.9
weight_decay: 0.005  
snapshot: 5000
snapshot_prefix: "newModel_stateFarm"
solver_mode: CPU
device_id: 0 
random_seed: 1701
